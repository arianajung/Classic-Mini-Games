<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".loginsystem.createaccount.CreateAccountActivity"></activity>
        <activity android:name=".spaceshooter.shootergameover.ShooterGameOver" />
        <activity android:name=".spaceshooter.shooterplanegame.ShooterGame" />
        <activity android:name=".spaceshooter.shootergamesetting.ShooterSetting" />
        <activity android:name=".spaceshooter.shootergamestart.ShooterStart" />
        <activity android:name=".hangman.HangmanSetting" />
        <activity android:name=".hangman.HangmanStageEnded" />
        <activity android:name=".hangman.HangmanGameActivity" />
        <activity android:name=".hangman.HangmanMain" />
        <activity android:name=".flappyfish.flappygameactivities.FlappyGameMenu" />
        <activity android:name=".flappyfish.flappygameactivities.FlappyResultActivity" />
        <activity android:name=".flappyfish.flappygameactivities.FlappySettingActivity" />
        <activity android:name=".flappyfish.flappygameactivities.FlappyMainActivity" />
        <activity android:name=".loginsystem.login.LoginActivity" />
        <activity android:name=".mainpage.GameMain" />
        <activity android:name=".loginsystem.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".flappyfish.flappygamemusic.FlappyBackgroundMusic" />
        <service android:name=".hangman.HangmanBackgroundMusic" />
        <service android:name=".spaceshooter.ShooterBackgroundMusic" />
    </application>

</manifest>